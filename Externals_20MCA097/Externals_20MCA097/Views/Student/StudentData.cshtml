@using Externals_20MCA097.Models
@model List<Student>
<div class="container">
    <h1> List of Students</h1>
    <table class="table table-dark">
        <tr>
            <td>ID</td>
            <td>NAME</td>
            <td>Course</td>
            <td>Year</td>
            <td>Gender</td>
        </tr>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Model[i].id</td>
                <td>@Model[i].fname</td>
                <td>@Model[i].course</td>
                <td>@Model[i].year</td>
                <td>@Model[i].Gender</td>
            </tr>
        }
    </table>
</div>